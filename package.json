{
  "name": "learn-starter",
  "version": "0.1.0",
  "engines" : {
    "node" : "14.0.0"
  },
  "main": "server.js",
  "private": true,
  "scripts": {
    "dev": "npx nodemon --exec npx babel-node server.js",
    "clean": "rm -rf build && mkdir build",
    "build": "next build && cross-env NODE_ENV=production babel server -d build",
    "start": "cross-env NODE_ENV=production node server.js"
  },
  "dependencies": {
    "@babel/runtime": "7.9.2",
    "@fortawesome/fontawesome-svg-core": "1.2.28",
    "@fortawesome/free-brands-svg-icons": "5.13.0",
    "@fortawesome/free-regular-svg-icons": "5.13.0",
    "@fortawesome/free-solid-svg-icons": "5.13.0",
    "@fortawesome/react-fontawesome": "0.1.9",
    "@zeit/next-sass": "1.0.1",
    "body-parser": "1.19.0",
    "cross-env": "7.0.2",
    "express": "4.17.1",
    "mongodb": "3.5.6",
    "mongoose": "5.9.10",
    "next": "9.3.5",
    "nextjs-fonts": "0.22.0",
    "node-sass": "4.14.0",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/node": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-react": "7.9.4",
    "axios": "0.19.2",
    "nodemon": "2.0.3"
  }
}
